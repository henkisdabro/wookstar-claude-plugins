    {
      "name": "mcp-fetch",
      "source": "./mcp-servers/fetch",
      "description": "Web content fetching and HTTP requests",
      "keywords": ["mcp", "fetch", "web", "http"],
      "category": "mcpServers",
      "mcpServers": {
        "fetch": {
          "command": "uvx",
          "args": ["mcp-server-fetch"]
        }
      },
      "strict": false
    },
    {
      "name": "mcp-google-workspace",
      "source": "./mcp-servers/google-workspace",
      "description": "Google Workspace integration (Gmail, Drive, Calendar)",
      "keywords": ["mcp", "google", "workspace", "gmail", "drive"],
      "category": "mcpServers",
      "mcpServers": {
        "google-workspace": {
          "command": "uvx",
          "args": ["workspace-mcp"],
          "env": {
            "GOOGLE_OAUTH_CLIENT_ID": "${GOOGLE_OAUTH_CLIENT_ID}",
            "GOOGLE_OAUTH_CLIENT_SECRET": "${GOOGLE_OAUTH_CLIENT_SECRET}",
            "OAUTHLIB_INSECURE_TRANSPORT": "1"
          }
        }
      },
      "strict": false
    },
    {
      "name": "mcp-mikrotik",
      "source": "./mcp-servers/mikrotik",
      "description": "MikroTik router management and network automation",
      "keywords": ["mcp", "mikrotik", "router", "network"],
      "category": "mcpServers",
      "mcpServers": {
        "mikrotik": {
          "command": "uvx",
          "args": [
            "mcp-server-mikrotik",
            "--host",
            "${MIKROTIK_HOST}",
            "--username",
            "${MIKROTIK_USER}",
            "--password",
            "${MIKROTIK_PASSWORD}",
            "--port",
            "2200"
          ]
        }
      },
      "strict": false
    },
    {
      "name": "mcp-n8n",
      "source": "./mcp-servers/n8n",
      "description": "n8n workflow automation integration",
      "keywords": ["mcp", "n8n", "automation", "workflows"],
      "category": "mcpServers",
      "mcpServers": {
        "n8n": {
          "command": "npx",
          "args": ["n8n-mcp"],
          "env": {
            "MCP_MODE": "stdio",
            "LOG_LEVEL": "error",
            "DISABLE_CONSOLE_OUTPUT": "true",
            "N8N_API_URL": "https://n8n.wookstar.com/",
            "N8N_API_KEY": "${N8N_API_KEY}"
          }
        }
      },
      "strict": false
    },
